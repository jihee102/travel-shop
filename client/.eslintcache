e.payload.isAuth){\r\n                        props.history.push(\"/\")\r\n                    }\r\n                    if(!option){\r\n                        props.history.push(\"/\")\r\n\r\n                    }\r\n                }\r\n            } )\r\n           \r\n        }, []);\r\n\r\n        return (<SpecificComponent {...props} user={user} />)\r\n    }\r\n\r\n\r\n\r\n\r\n    return AuthenticationCheck;\r\n\r\n}","D:\\Jihee file\\travelShop\\client\\src\\reducers\\chat_reducer.js",[],"D:\\Jihee file\\travelShop\\client\\src\\reducers\\user_reducer.js",["193"],"\r\nimport { LOGIN_USER, REGISTER_USER,AUTH_USER,LOGOUT_USER } from '../actions/types';\r\n\r\nexport default function(state={},action){\r\n    switch(action.type){\r\n        case REGISTER_USER:\r\n            return {...state, register: action.payload }\r\n        case LOGIN_USER:\r\n            return { ...state, loginSucces: action.payload }\r\n        case AUTH_USER:\r\n            return {...state, userData: action.payload }\r\n        case LOGOUT_USER:\r\n            return {...state }\r\n        default:\r\n            return state;\r\n    }\r\n}","D:\\Jihee file\\travelShop\\client\\src\\components\\about\\index.js",[],"D:\\Jihee file\\travelShop\\client\\src\\actions\\user_action.js",[],"D:\\Jihee file\\travelShop\\client\\src\\actions\\types.js",[],"D:\\Jihee file\\travelShop\\client\\src\\components\\navbar\\NavBar.js",[],"D:\\Jihee file\\travelShop\\client\\src\\components\\footer\\Footer.js",[],"D:\\Jihee file\\travelShop\\client\\src\\components\\navbar\\sections\\LeftMenu.js",["194","195"],"  \r\nimport React from 'react';\r\nimport { Menu } from 'antd';\r\nconst SubMenu = Menu.SubMenu;\r\nconst MenuItemGroup = Menu.ItemGroup;\r\n\r\nfunction LeftMenu(props) {\r\n  return (\r\n    <Menu mode={props.mode}>\r\n      <Menu.Item key='mail'>\r\n        <a href='/'>Home</a>\r\n      </Menu.Item>\r\n      <Menu.Item key='favorite'>\r\n        <a href='/favorite'>Favorite</a>\r\n      </Menu